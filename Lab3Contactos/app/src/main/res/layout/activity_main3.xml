<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Main3Activity">

    <TextView
            android:text="Contacto:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView" android:textSize="30sp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="16dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="127dp"
            android:layout_marginStart="127dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="126dp" android:layout_marginRight="126dp" android:layout_marginBottom="118dp"
            app:layout_constraintBottom_toTopOf="@+id/tvNameXML"/>
    <TextView
            android:text="Name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvNameXML" android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="160dp" android:layout_marginStart="160dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="160dp"
            android:layout_marginRight="160dp" android:layout_marginTop="117dp"
            app:layout_constraintTop_toBottomOf="@+id/textView"/>
    <TextView
            android:text="Cellphone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvCellphoneXML" android:textSize="24sp"
            android:textColor="@android:color/holo_blue_dark" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="139dp" android:layout_marginRight="139dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="140dp"
            android:layout_marginStart="140dp" android:layout_marginTop="33dp"
            app:layout_constraintTop_toBottomOf="@+id/tvNameXML"/>
    <TextView
            android:text="Email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvEmailXML" android:textSize="24sp"
            android:textColor="@android:color/holo_blue_dark" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="160dp" android:layout_marginStart="160dp" android:layout_marginTop="43dp"
            app:layout_constraintTop_toBottomOf="@+id/tvCellphoneXML" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="164dp" android:layout_marginRight="164dp" android:layout_marginBottom="15dp"
            app:layout_constraintBottom_toTopOf="@+id/button"/>
    <Button
            android:text="Regresar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="55dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="143dp"
            android:layout_marginRight="143dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="148dp" android:layout_marginStart="148dp" android:layout_marginTop="62dp"
            app:layout_constraintTop_toBottomOf="@+id/tvEmailXML"/>
</android.support.constraint.ConstraintLayout>